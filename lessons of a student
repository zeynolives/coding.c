    cin>>algo;
    cout<<"enter phy:";
    cin>>physic;
  void DisplayInfo(){
     cout<<"math:"<<math;
     cout<<"algo:"<<algo;
     cout<<"physic:"<<physic;
  }
 int average(){
         int res=(math+algo+physic)/3;
         return res;
  }
  void DisplayAverage(int res){
     cout<<"sudent average:"<<averag<<endl;
  }
  void Compare(Student S1){
        
        if(this->avg()>S1.avg()){
            cout<<"S1 is bigger than S3";
        }
        else{
            cout<<"s1 is less than s3";
        }
  }
};
int main()
{
    Student Student1();
